<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data></data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout_studentid"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/tv_department_where"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="68dp"
            android:layout_marginEnd="182dp"
            android:text="@string/onboarding_tv_what_department"
            android:textAppearance="?textAppearanceHeadline2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/et_department_search"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/shape_grayscales400_fill_8_rect"
            android:drawableEnd="@drawable/ic_onboarding_search"
            android:paddingTop="16dp"
            android:paddingEnd="12dp"
            android:paddingBottom="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_department_where" />

        <TextView
            android:id="@+id/tv_studentid_what"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="76dp"
            android:layout_marginEnd="182dp"
            android:text="@string/onboarding_tv_what_studentid"
            android:textAppearance="?textAppearanceHeadline2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@id/et_department_search" />

        <TextView
            android:id="@+id/et_studentid_search"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="12dp"
            android:text="@string/onboarding_hint_select_studentid"
            android:textColor="@color/grayscales_500"
            android:fontFamily="@font/font_pretendard_medium"
            android:layout_marginEnd="16dp"
            android:paddingHorizontal="20dp"
            android:background="@drawable/shape_grayscales500_fill_8_rect"
            android:paddingTop="16dp"
            android:paddingEnd="12dp"
            android:textSize="16sp"
            android:paddingBottom="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_studentid_what"
            app:drawableEndCompat="@drawable/ic_onboarding_studentid_down" />

        <android.widget.Button
            android:id="@+id/btn_stdentid_next"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="34dp"
            android:fontFamily="@font/font_pretendard_medium"
            android:background="@drawable/shape_white_fill_8_rect"
            android:backgroundTint="@color/black"
            android:paddingVertical="12dp"
            android:text="@string/onboarding_btn_next"
            android:textColor="@color/yello_main_500"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>